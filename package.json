{
    "name": "biz_app",
    "version": "1.0.0",
    "description": "Web app for Merchandisers  and retailers",
    "main": "index.js",
    "scripts": {
        "help": "webpack --help",
        "dev": "webpack --mode development ./frontend/src/index.js --output-path ./frontend/static/js/main/",
        "start:retailer": "webpack serve --config webpack.dev.config.js --progress --color",
        "dev_prod": "webpack --mode development ./retailer/src/index.js --output-path ./retailer/static/js/view/",
        "watch": "webpack --mode development --watch ./frontend/src/index.js --output-path ./frontend/static/js/main/",
        "watch_prod": "webpack --mode development --watch ./retailer/src/index.js --output-path ./retailer/static/js/view/",
        "build": "webpack --mode production ./frontend/src/index.js --output-path ./frontend/static/js/main/",
        "build_prod": "webpack --mode production ./retailer/src/index.js --output-path ./retailer/static/js/view/",
        "production": "concurrently \"npm run build\" \"npm run build_prod\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Netbotgroup/biz_app.git"
